                                      ['公共事业、旅游、文化教育类', 9], ['艺术设计传媒大类', 10], ['公安、法律类 ', 11], ['其他', 12]],as: :radio_buttons, include_blank: false
= f.input :position, collection: [['东部地区', 1], ['中部地区', 2], ['西部地区', 3]],as: :radio_buttons, include_blank: false
= f.input :school_position, collection: [['乡镇', 1], ['县城', 2], ['地级市', 3], ['省会城市', 4], ['直辖市', 5]],as: :radio_buttons, include_blank: false
= f.input :known, collection: [['是', 1], ['否', 2], ['不知道', 3]],as: :radio_buttons, include_blank: false
